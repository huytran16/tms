@{
    ViewBag.Title = "Provider";
    Layout = "~/Views/Service/_ServiceLayout.cshtml";


    <link href="~/Content/css/thumbnails.css" rel="stylesheet" />

}

<div class="col-md-12">
    <div class="panel panel-default">
        <div class="panel-heading panel-heading-gray">Tìm kiếm dịch vụ</div>
        <div class="panel-body">
            <form class="form-horizontal" role="form">
                <div class="form-group">
                    <label class="col-sm-3 control-label">Loại dịch vụ</label>
                    <div class="col-sm-9">
                        <select name="select" class="selectpicker" data-size="3">
                            <option>Dịch vụ internet</option>
                            <option>Dịch vụ sữa chữa</option>
                            <option>Khác</option>
                        </select>
                    </div>
                </div>
                <div class="form-group">

                    <div class="col-sm-offset-3 col-md-2">
                        <button type="submit" class="btn btn-primary">Tìm kiếm</button>
                    </div>

                </div>


            </form>
        </div>
    </div>
